#include "loops.h"
#include <math.h>
/**
 *sqr - squares the input number
 *@a: input
 *Return: double
 */
double sqr(int a)
{
	double sqr;

	sqr = a * a;
	return (sqr);
}

/**
 *sqroot - squareroot of input
 *@a: input
 *Return: double
 */
double sqroot(int a)
{
	double _sqroot;

	_sqroot = sqrt(a);
	return (_sqroot);
}
